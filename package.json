{
  "name": "geoguru-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material/data-table": "^4.0.0",
    "autoprefixer": "^9.7.3",
    "live-server": "^1.2.1",
    "material-components-web": "^4.0.0",
    "postcss": "^8.2.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "body-parser": "^1.18.3",
    "connect": "^3.6.6",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "errorhandler": "^1.5.0",
    "events": "^3.0.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nconf": "^0.10.0",
    "random": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.2.0",
    "winston": "^3.1.0",
    "yargs": "^12.0.5"
  },
  "scripts": {
    "start:sass": "npm-run-all --parallel devserver watch:sass",
    "watch:sass": "node-sass src/sass/main.scss src/App.css -w --include-path node_modules/",
    "devserver": "live-server --open=/public",
    "react:start": "react-scripts start",
    "start": "node src/server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "supervisor": "^0.12.0"
  }
}
